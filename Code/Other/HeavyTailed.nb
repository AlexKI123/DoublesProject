     RowBox[{
      RowBox[{"cdf", "[", "x", "]"}], "^", 
      RowBox[{"(", 
       RowBox[{"i", "-", "1"}], ")"}]}], "*", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"cdf", "[", "x", "]"}]}], ")"}], "^", 
      RowBox[{"(", 
       RowBox[{"n", "-", "i"}], ")"}]}], "*", 
     RowBox[{"pdf", "[", "x", "]"}]}]}], ";"}]}], "\[IndentingNewLine]", 
